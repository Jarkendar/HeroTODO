<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jarek.questtemporary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activityClasses.QuestPanelMain"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".activityClasses.QuestForm" />
        <activity android:name=".activityClasses.OptionActivity" />
        <activity android:name=".activityClasses.StatisticActivity" />
        <activity android:name=".activityClasses.HelpActivity" />
        <activity android:name=".activityClasses.launcher">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN"
                    android:screenOrientation="portrait" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activityClasses.TutorialActivity"></activity>
    </application>

</manifest>